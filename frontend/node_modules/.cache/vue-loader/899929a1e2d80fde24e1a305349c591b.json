in: auto;\n  padding: 0.5em;\n  background: rgb(255,255,255);\n}\n\n\n.dailyByDate{\n  width: 70%;\n  margin: 1em auto;\n  display: grid;\n  grid-template-columns: 60% 40%;\n  background: rgb(158, 243, 196);\n  margin-bottom: 1em;\n}\n.dailyByDate form{\n  margin-top: 2em;\n}\n.dailyByName{\n  width: 70%;\n  margin: 1em auto;\n  display: grid;\n  grid-template-columns: 60% 40%;\n   background: rgb(158, 243, 196);\n  margin-bottom: 1em;\n}\n.dailyByName form{\n  margin-top: 1.5em;\n}\n.dailyByCode{\n  width: 70%;\n  margin: 1em auto;\n  display: grid;\n  grid-template-columns: 60% 40%;\n   background: rgb(158, 243, 196);\n  margin-bottom: 1em;\n}\n.dailyByCode form{\n  margin-top: 1.5em;\n}\nlabel{\n  padding-left: 0.5em;\n  font-size: small;\n  color: var(--main-dark-grey);\n}\n</style>\n"]}]}