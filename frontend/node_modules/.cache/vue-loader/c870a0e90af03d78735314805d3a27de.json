span>getDailyReportAllCountries <i class=\"fas fa-lock\"></i>\n            </li>\n          </ul>\n        </div>\n\n        <div class=\"response-box\">\n          <div class=\"response-description\">\n            <p>Description:  {{apiResponse.description}}</p>\n          </div>\n          <hr>\n          <div class=\"response-data\"><p>{{apiResponse.data}}</p></div>\n        </div>\n      \n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\nimport Header from \"../Header\"\n\nexport default {\n  name: \"ApiPage\",\n  components: {Header},\n  props: [\"myProp\"],\n  data() {\n    return {};\n  },\n\n  computed: mapGetters([\"apiResponse\", \"processing\"]),\n  methods: {\n    ...mapActions([\n      \"getListOfCountries\",\n      \"OpenAPIDocumentation\",\n      \"getLatestTotals\",\n      \"getDailyReportTotals\",\n      \"getDailyReportByCountryName\",\n      \"getLatestAllCountries\",\n      \"getLatestCountryDataByCode\",\n      \"getLatestCountryDataByName\",\n      \"getDailyReportByCountryCode\",\n      \"getDailyReportAllCountries\"\n    ]),\n\n    getImgUrl(){\n      return \"https://rapidapi-prod-collections.s3.amazonaws.com/50ac3a55-4378-4965-a9a4-80bc9d05ac7a.jpg\"\n    }\n  },\n  \n\n  created() {}\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n#apiPage{\n  background-image: url('https://images.pexels.com/photos/531880/pexels-photo-531880.jpeg?auto=compress&cs=tinysrgb&dpr=3&h=750&w=1260');\n  background-size: cover;\n}\np{\n  border: none;\n}\n.box {\n  width: 80vw;\n  min-height: 100vh;\n  margin: auto;\n  background: rgb(255, 255, 255);\n}\nh3{\n  background: white;\n  line-height: 4em;\n  text-align: center;\n  color: var(--main-blue);\n  text-transform: uppercase;\n}\n.content-box {\n  min-height: 90vh;\n  display: grid;\n  grid-template-columns: 30% 70%;\n}\n.request-box {\n  padding: 0.5em;\n  border: 1px solid #ccc;\n}\n.response-box {\n  border: 1px solid #ccc;\n}\nul {\n  list-style-type: none;\n}\n.request-box ul {\n  margin-top: 2em;\n}\n.request-box li {\n  color:rgb(41, 90, 224);\n  cursor: pointer;\n  border: 1px solid rgb(230, 229, 229);\n  background: rgb(247, 207, 121);\n  border-radius: 5px;\n  width: 90%;\n  height: 2em;\n  line-height: 2em;\n  margin: 2px 2px 2px 4px;\n}\n.response-box {\n  border: 1px solid #ccc;\n}\n.request-type {\n  color: rgb(33, 184, 58);\n  border: 1px solid rgb(173, 173, 173);\n  border-radius: 5px;\n  background: #ccc;\n  padding: 3px;\n  margin: 0 5px 0 2px;\n}\n.processing{\n  display: block;\n  height: 1em;\n}\n.processing p{\n  padding: 0;\n  margin: 0;\n  font-size: small;\n  line-height: 1em;\n  border: none;\n}\n.covid-19-logo span {\n  color:rgb(224, 41, 41);\n  font-size: xx-large;\n}\n.covid-19-logo a {\n  text-decoration: none;\n}\n.response-description{\n  padding: 0.5em;\n  color: rgb(82, 80, 80);\n}\n.response-data{\n  padding: 0.5em;\n  color: rgb(146, 145, 145);\n}\n\n</style>\n"]}]}